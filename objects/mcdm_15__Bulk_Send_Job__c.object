        <fullName>mcdm_15__Status__c</fullName>
        <deprecated>false</deprecated>
        <description>The status of the bulk send job, Not Started, Processing, Waiting, Sending, Completed</description>
        <externalId>false</externalId>
        <formula>IF(mcdm_15__Total_Chunks__c = 0, $Label.mcdm_15__csvJobStatusNotStarted, IF( mcdm_15__Chunks_Retry__c &gt; 0, $Label.mcdm_15__bulkJobStatusRetry, IF( mcdm_15__Chunks_Completed__c=mcdm_15__Total_Chunks__c, $Label.mcdm_15__csvJobStatusCompleted,  IF( AND(mcdm_15__Chunks_Waiting__c = mcdm_15__Total_Chunks__c, mcdm_15__Is_Sendable__c) , $Label.mcdm_15__csvJobStatusWaiting,  IF( mcdm_15__Chunks_Sending__c &gt; 0, $Label.mcdm_15__csvJobStatusSending,  IF( AND(OR(mcdm_15__Chunks_Processing__c &gt; 0, mcdm_15__Chunks_Waiting__c &gt; 0), NOT(mcdm_15__Is_Sendable__c)),  $Label.mcdm_15__csvJobStatusProcessing, $Label.mcdm_15__csvJobStatusNotStarted) ) ) ) ) )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Status</label>
        <required>false</required>
