    <fields>
        <fullName>Dependency_Status__c</fullName>
        <inlineHelpText>Status of the dependency between two User Stories. Example: Prioritized, Committed, or Completed. You can manually edit this field. However, it will be automatically marked as Completed when you mark the Parent User Story to Completed.</inlineHelpText>
        <label>Dependency Status</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
        <valueSet>
            <restricted>true</restricted>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>New</fullName>
                    <default>true</default>
                    <label>New</label>
                </value>
                <value>
                    <fullName>More Info Needed</fullName>
                    <default>false</default>
                    <label>More Info Needed</label>
                </value>
                <value>
                    <fullName>Prioritized</fullName>
                    <default>false</default>
                    <label>Prioritized</label>
                </value>
                <value>
                    <fullName>Committed</fullName>
                    <default>false</default>
                    <label>Committed</label>
                </value>
                <value>
                    <fullName>Completed</fullName>
                    <default>false</default>
                    <label>Completed</label>
                </value>
                <value>
                    <fullName>Deferred</fullName>
                    <default>false</default>
                    <label>Deferred</label>
                </value>
                <value>
                    <fullName>Never</fullName>
                    <default>false</default>
                    <label>Never</label>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>
