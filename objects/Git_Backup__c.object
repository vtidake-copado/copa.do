			Git_Repository__r.Branch_Base_URL__c +  Branch__c,
			IF(
				CONTAINS(Git_Repository__r.Branch_Base_URL__c,&apos;[BRANCH_NAME]&apos;),
				SUBSTITUTE(Git_Repository__r.Branch_Base_URL__c,&apos;[BRANCH_NAME]&apos;,Branch__c),
				Git_Repository__r.Branch_Base_URL__c + Branch__c
			)
		),
	&apos;&apos;), 
	Branch__c
)</formula>
