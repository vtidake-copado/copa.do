    <validationRules>
        <fullName>vlocity_ins__MedicationFrequencyCheck</fullName>
        <active>true</active>
        <description>This rule checks if the Frequency (Other) field is also set, if the Frequency field has been set to &apos;other&apos;.</description>
        <errorConditionFormula>AND((ISPICKVAL(vlocity_ins__Frequency__c , &quot;other&quot;)), ISBLANK(vlocity_ins__FrequencyOther__c))</errorConditionFormula>
        <errorDisplayField>vlocity_ins__FrequencyOther__c</errorDisplayField>
        <errorMessage>Frequency (Other) must not be empty, if the Frequency chosen is &apos;other&apos;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>vlocity_ins__MedicationMustBeActiveOrStoppedRule</fullName>
        <active>true</active>
        <description>This rule checks that the Medication is either has StopReason set OR EndDate set to now or earlier; otherwise it should be marked Active.</description>
        <errorConditionFormula>AND(NOT(vlocity_ins__IsActive__c), ISBLANK(TEXT(vlocity_ins__StopReason__c)), OR(ISNULL(vlocity_ins__MedicationEndDate__c), vlocity_ins__MedicationEndDate__c  &gt; TODAY()))</errorConditionFormula>
        <errorDisplayField>vlocity_ins__IsActive__c</errorDisplayField>
        <errorMessage>The medication must have the Active field set. Otherwise, either the Stop Reason field needs to be set OR the Medication End Date field must be set to current date or earlier.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>vlocity_ins__MedicationMustNotBeStoppedIfActiveRule</fullName>
        <active>true</active>
        <description>This rule checks that the Medication has not been stopped or has an end date today or earlier, if it is still active.</description>
        <errorConditionFormula>AND (vlocity_ins__IsActive__c, OR(NOT(ISBLANK(TEXT( vlocity_ins__StopReason__c ))),  vlocity_ins__MedicationEndDate__c  &lt;=  TODAY()))</errorConditionFormula>
        <errorDisplayField>vlocity_ins__IsActive__c</errorDisplayField>
        <errorMessage>If the Medication is marked Active, neither the Stop Reason field nor the Medication End Date field can be set.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
