    <validationRules>
        <fullName>API_Name_Cannot_Contain_Spaces</fullName>
        <active>true</active>
        <errorConditionFormula>CONTAINS(API_Name__c, &apos; &apos;)</errorConditionFormula>
        <errorDisplayField>API_Name__c</errorDisplayField>
        <errorMessage>The API Name cannot contain blank spaces.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Lookups_empty_for_Global_Property</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
Is_Global__c = True,
OR(
NOT(ISBLANK(User__c)),
NOT(ISBLANK(Pipeline__c)),
NOT(ISBLANK(Environment__c))
))</errorConditionFormula>
        <errorMessage>The environment, pipeline, and user lookups must be empty if the &apos;Is Global&apos; checkbox is selected.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Only_One_Parent_Is_Allowed</fullName>
        <active>true</active>
        <errorConditionFormula>OR(
AND(
NOT(ISBLANK(Environment__c)),
NOT(ISBLANK(Pipeline__c))
),
AND(
NOT(ISBLANK(Environment__c)),
NOT(ISBLANK(User__c))
),
AND(
NOT(ISBLANK(User__c)),
NOT(ISBLANK(Pipeline__c))
)
)</errorConditionFormula>
        <errorMessage>A non-global property can only be related to one parent: Environment, Pipeline, or User.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Value_cannot_be_empty</fullName>
        <active>true</active>
        <description>System property record must have a value</description>
        <errorConditionFormula>ISBLANK(Value__c)</errorConditionFormula>
        <errorMessage>You must provide a value for this system property.</errorMessage>
    </validationRules>
