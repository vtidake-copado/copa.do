            "%vlocity_namespace%__CSS__c": ".vlocity .account-story .spacer {\n  background-color: white;\n  height: 40px;\n  box-sizing: border-box;\n  text-transform: uppercase;\n  text-align: left;\n  font-size: 12px;\n  color: black;\n  box-sizing: border-box;\n  padding: 9px;\n  width: inherit;\n  border-top: 2px solid #e6e6ec;\n  border-bottom: 2px solid #e6e6ec;\n  cursor: pointer; }\n\n.vlocity .account-story .spacer:hover {\n  color: #05a6df; }\n\n.vlocity .account-story .spacer i::before {\n  color: #1397db;\n  font-size: 14px; }\n\n.vlocity .account-story #story-time {\n  background-color: white; }\n\n.vlocity .account-story .story-type {\n  font-size: 12px;\n  margin-left: 5px; }\n\n.vlocity .account-story .story-numberOfDays {\n  font-size: 12px;\n  margin-left: 5px;\n  margin-top: 10px;\n  padding-left: 0; }\n\n.vlocity .account-story .story-numberOfDays label {\n  display: inline; }\n\n.vlocity .account-story .story {\n  font-family: 'Helvetica Neue', Helvetica, Arial, sans-serif;\n  font-size: 11px;\n  /*position: relative;\n        height: 65px;*/\n  position: static;\n  height: auto;\n  width: 100%;\n  padding: 10px 0;\n  cursor: pointer; }\n  .vlocity .account-story .story:hover {\n    background-color: #f2f2f2; }\n  .vlocity .account-story .story.open-oppty {\n    background-color: rgba(0, 164, 83, 0.2); }\n  .vlocity .account-story .story.open-case {\n    background-color: rgba(235, 33, 46, 0.2); }\n\n.vlocity .account-story #story-time .story .line {\n  height: 40px;\n  width: 1px;\n  background-color: #ccc;\n  color: black; }\n\n.vlocity .account-story #story-time .story:last-child .line {\n  height: 0;\n  width: 0px; }\n\n.vlocity .account-story .icon-segment {\n  float: left;\n  position: relative;\n  /* added */ }\n  .vlocity .account-story .icon-segment .story-icon {\n    font-size: 24px;\n    line-height: 24px;\n    color: #05a6df;\n    width: 30px; }\n    .vlocity .account-story .icon-segment .story-icon:before {\n      font-weight: 100;\n      line-height: 24px; }\n\n.vlocity .account-story .icon-segment img {\n  position: absolute;\n  left: 9px;\n  top: 11px;\n  height: 22px; }\n\n.vlocity .account-story .icon-segment i::before {\n  color: #1397db; }\n\n.vlocity .account-story .icon-segment .line {\n  position: absolute;\n  left: 20px;\n  top: 35px;\n  z-index: 1; }\n\n.vlocity .account-story #story-time .story .story-info {\n  /*position:absolute;\n        left: 40px;*/\n  position: relative;\n  margin-left: 40px;\n  width: 80%; }\n  .vlocity .account-story #story-time .story .story-info div + div:not(.story-date) {\n    font-size: 11px; }\n  .vlocity .account-story #story-time .story .story-info > div:first-of-type strong, .vlocity .account-story #story-time .story .story-info .story-date {\n    font-size: 11px; }\n  .vlocity .account-story #story-time .story .story-info div {\n    text-transform: capitalize; }\n  .vlocity .account-story #story-time .story .story-info .story-type {\n    text-transform: uppercase;\n    padding-bottom: 5px; }\n  .vlocity .account-story #story-time .story .story-info .story-date {\n    position: absolute;\n    right: 0px;\n    top: 0px; }\n  @media (max-width: 300px) {\n    .vlocity .account-story #story-time .story .story-info .story-date {\n      position: static; } }",
            "%vlocity_namespace%__DisplayOn__c": "All",
            "%vlocity_namespace%__HTML__c": "<div class=\"account-story\">\n    <div id=\"story-time\">\n      <div class=\"story-type\">\n            <label>Type:</label>\n            <select ng-model=\"importedScope.selectedStoryType\"\n                    ng-options=\"storyType for storyType in importedScope.uniqueStoryTypeList | orderBy:storyType\"\n                    ng-change=\"importedScope.change()\">\n                <option value=\"\">All</option>\n            </select>\n      </div>\n      <div class=\"story-numberOfDays col-md-2\">\n            <label>Number of Days:</label>\n            <input ng-model=\"importedScope.numberOfDays\" type=\"text\" maxlength=\"7\" size=\"7\" ng-change=\"importedScope.change()\"/>\n      </div>\n      <div class=\"story col-md-12\" ng-repeat=\"story in importedScope.stories track by $index\">\n        <div class=\"icon-segment\">\n          <img src=\"{{story.imageRef}}\">\n          <div class=\"line\"></div>\n        </div>\n        <div class=\"story-info\">\n          <div class=\"story-type\"><strong>{{story.objType}}</strong></div>\n          <div>{{story.title}}</div>\n          <div>Account: {{story.parentName}}</div>\n          <div class=\"story-date\">{{story.formatedLastActivityDate}}</div>\n        </div>\n      </div>\n    </div>\n</div>",
            "%vlocity_namespace%__Sass__c": "$light-blue: #05a6df;\n.vlocity {\n    .account-story {\n    \n    .spacer {\n        background-color: rgb(255,255,255);\n        height: 40px;\n        box-sizing: border-box;\n        text-transform: uppercase;\n        text-align: left;\n        font-size: 12px;\n        color: rgb(0,0,0);\n        box-sizing: border-box;\n        padding: 9px;\n        width: inherit;\n        border-top: 2px solid rgba(230,230,236,1);     \n        border-bottom: 2px solid rgba(230,230,236,1); \n        cursor: pointer;\n    }\n    .spacer:hover {\n        color: #05a6df;\n    }\n    .spacer i::before {\n        color: rgb(19, 151, 219);\n        font-size: 14px;\n    }\n    \n    #story-time {\n        background-color: white;\n    }\n    .story-type {\n        font-size: 12px;\n        margin-left: 5px;\n    }\n    .story-numberOfDays {\n        font-size: 12px;\n        margin-left: 5px;\n        margin-top: 10px;\n        padding-left: 0;\n    }\n    .story-numberOfDays label {\n        display: inline;\n    }\n    .story {\n        font-family: 'Helvetica Neue', Helvetica, Arial, sans-serif;\n        font-size: 11px;\n        \n        /*position: relative;\n        height: 65px;*/\n        position: static;\n        height: auto;\n        \n        width: 100%;\n        padding: 10px 0;\n        cursor: pointer;\n\n        &:hover {\n            background-color: #f2f2f2;\n        }\n        \n        &.open-oppty {\n            background-color: rgba(0, 164, 83, 0.2);\n        }\n        \n        &.open-case {\n            background-color: rgba(235, 33, 46, 0.2);\n        }\n    }\n    #story-time .story .line {\n        height: 40px;\n        width: 1px;\n        background-color: #ccc;\n        color: rgb(0,0,0);\n    }\n    #story-time .story:last-child .line {\n        height: 0;\n        width: 0px;\n    }\n    .icon-segment {\n        float:left;\n        position: relative; /* added */\n        .story-icon {\n            font-size: 24px;\n            line-height: 24px;\n            color: $light-blue;\n            width: 30px;\n        \n            &:before {\n                font-weight: 100;\n                line-height: 24px;\n            }\n        }\n    }\n    .icon-segment img {\n        position: absolute;\n        left: 9px;\n        top: 11px;\n        height: 22px;\n    }\n    .icon-segment i::before  {\n        color: rgb(19, 151, 219);\n    }\n    .icon-segment .line {\n        position: absolute;\n        left: 20px;\n        top: 35px;\n        z-index: 1;\n    }\n    #story-time .story .story-info {\n        \n        /*position:absolute;\n        left: 40px;*/\n        position: relative;\n        margin-left: 40px;\n        \n        width: 80%;\n        div + div:not(.story-date) {\n            font-size: 11px;\n        }\n        &>div:first-of-type strong, .story-date {\n            font-size: 11px;\n        }\n        div {\n            text-transform: capitalize;\n        }\n        .story-type {\n            text-transform: uppercase;\n            padding-bottom:5px;\n        }\n        .story-date {\n            position:absolute;\n            right: 0px;\n            top: 0px;\n        }\n        \n        // For Home Page Narrow component width\n        @media (max-width: 300px) {\n            .story-date {\n                position:static;\n            }\n        }\n    }\n    }\n}",
