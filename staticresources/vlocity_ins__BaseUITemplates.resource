            "%vlocity_namespace%__HTML__c": "<div class=\"profile\">\n    <div class=\"content\">\n        <div class=\"welcome-note\">{{data.title}}</div>\n        <h2>{{obj.Name}}</h2>\n        <div class=\"content-details hidden-xs\">\n            <ul class=\"details\" ng-show=\"data.fields.length == 0\">\n                <li class=\"detail\">\n                    <label class=\"detail-label\">Email</label>\n                    <div class=\"value\">{{obj['Email']}}</div>\n                </li>\n                <li class=\"detail\">\n                    <label class=\"detail-label\">Address</label>\n                    <div class=\"value\">{{obj['Address']['street']}}</div>\n                    <div class=\"value\">{{obj['Address']['city']}}, {{obj['Address']['state']}} {{obj['Address']['postalCode']}}</div>\n                </li>\n                <li class=\"detail\">\n                    <label class=\"detail-label\">Phone</label>\n                    <div class=\"value\">{{obj['Phone']}}</div>\n                </li>\n            </ul>\n            <ul class=\"details\" ng-show=\"data.fields\">\n                <li class=\"detail\" ng-repeat=\"field in data.fields\" ng-show=\"key != 'attributes'\">\n                    <label class=\"detail-label\">{{field.label}}</label>\n                    <div class=\"value\" ng-if=\"!obj[field.relationshipName]\">\n                        {{obj[field.name]}}\n                    </div>\n                    <div class=\"value\" ng-if=\"obj[field.relationshipName]\">{{obj[field.relationshipName]['Name']}}</div>\n                </li>\n            </ul>\n        </div>\n    </div>\n    <div class=\"actions\">\n        <ul>\n            <li ng-repeat=\"action in data.actions | limitTo: (data.limitActionLinksTo?data.limitActionLinksTo:5)\">\n                <a href=\"#\" ng-click=\"performAction(action)\">\n                    <i ng-if=\"!action.imageRef\" class=\"icon {{action.vlocityIcon}}\"></i>\n                    <span ng-if=\"action.imageRef\">\n                        <img ng-src=\"{{action.imageRef}}\" alt=\"{{action.displayName}}\"/>\n                    </span>\n                    <span class=\"action-title\">{{action.displayName}}</span>\n                </a>\n            </li>\n        </ul>\n    </div>\n</div>",
            "%vlocity_namespace%__Sass__c": "@import \"variables\";\n@import \"community-variables\";\n\n.vlocity {\n    .community {\n\n        /* Profile*/\n        .profile {\n            border:1px solid #dedede;\n            background-color: $white;\n            .content {\n                padding:10px  15px;\n                .welcome-note {\n                    color: $font-gray;\n                }\n                h2 {\n                    font-size: 25px;\n                    font-weight: bold;\n                    margin: 5px 0px 15px;\n                }\n                ul {\n                    margin:0;\n                    padding:0;\n                    &.details {\n                        li {\n                            padding-bottom:10px;\n                            list-style-type:none;\n                            .detail-label {\n                                text-transform: uppercase;\n                                margin-bottom:0;\n                                color:$font-gray;\n                            }\n                        }\n                    }\n                }\n            }\n            .actions {\n                ul {\n                    list-style-type: none;\n                    padding:0px;\n                    li {\n                        padding:10px 15px;\n                        border-top:1px solid #dedede;\n                        text-transform: uppercase;\n                        text-overflow: ellipsis;\n                        overflow: hidden;\n                        white-space: nowrap;\n                        color: #9a9a9a;\n                        i {\n                            color: #05A6DF;\n                            font-size:20px;\n                        }\n                        .action-title {\n                            line-height:20px;\n                            vertical-align: top;\n                            color: $font-gray;\n                            text-transform: uppercase;\n                        }\n                    }\n                }\n            }\n        }\n        @media (max-width: $tablet-width) {\n            .profile {\n                border:none;\n                background-color: transparent;\n                .content {\n                    color:$white;\n                    h2 {\n                        font-size:30px;\n                    }\n                    .welcome-note {\n                        color: $white;\n                    }\n                }\n                .actions {\n                    ul {\n                        background-color: #05A6DF;\n                        li {\n                            padding: 10px 5px;\n                            border-top:none;\n                            display:inline-block;\n                            text-align: center;\n                            color: $white;\n                            width: 24%;\n                            i {\n                                display: block;\n                                color: $white;\n                                font-size:40px;\n                            }\n                            .action-title {\n                                color: $white;\n                            }\n                        }\n                    }\n                }\n            }\n        }\n    }\n}",
