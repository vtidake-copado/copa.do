            "%vlocity_namespace%__HTML__c": "<div class=\"account-card active\" >\n    <div class=\"card-top\" ng-click=\"cardShowMore = !cardShowMore\">\n        <div class=\"rightActions showHideParent\" ng-class=\"{hideMore: cardShowMore}\">\n            <div class=\"showButton hidden-xs\">Show Details <i class=\"icon icon-v-down-caret\"></i></div>\n            <div class=\"hideButton hidden-xs\">Hide Details <i class=\"icon icon-v-up-caret\"></i></div>\n            <div class=\"actions visible-xs\" ng-click=\"displayActions = !displayActions;$event.stopPropagation();\"> <i class=\"icon icon-v-menu2\"></i></div>\n        </div>\n        <div>\n            <h1>{{obj.Name}}</h1>\n        </div>\n    </div>\n    <div class=\"card-info\">\n        <ul class=\"details\" ng-class=\"{'hidden-xs': !cardShowMore}\">\n            <li class=\"detail col-xs-12 col-sm-6 col-md-4 col-lg-4\" ng-repeat=\"field in data.fields\" ng-show=\"key != 'attributes'\">\n                <label class=\"detail-label\">{{field.label}}</label>\n                <div class=\"value\" ng-if=\"!obj[field.relationshipName]\">\n                    {{obj | getter: field | picker: field.type}}\n                </div>\n            </li>\n        </ul>\n        <div class=\"actions-wrapper\" ng-class=\"{'hidden-xs': !displayActions}\">\n            <div class=\"actions-container\">\n                <ul class=\"actions\">\n                    <li ng-repeat=\"action in data.actions | limitTo: (data.limitActionLinksTo?data.limitActionLinksTo:5)\" ng-class=\"{last:$last}\">\n                        <a href=\"#\" ng-click=\"performAction(action)\">\n                            <i ng-if=\"!action.imageRef\" class=\"icon {{action.vlocityIcon}}\"></i>\n                            <span ng-if=\"action.imageRef\">\n                                <img ng-src=\"{{action.imageRef}}\" alt=\"{{action.displayName}}\">\n                            </span>\n                            <span class=\"title\">{{action.displayName}}</span>\n                        </a>\n                    </li>\n                </ul>\n                <div class=\"actions-cancel visible-xs\" ng-click=\"displayActions = !displayActions;$event.stopPropagation();\">CANCEL</div>\n            </div>\n            <div class=\"page-cover-layer\" ng-class=\"{'hidden-xs': !displayActions}\"></div>\n        </div>\n        \n        <vloc-layout layout-name=\"{{data.flyout.layout}}\" ng-if=\"data.flyout.layout && cardShowMore\" parent=\"data\"></vloc-layout>\n    </div>\n</div>",
            "%vlocity_namespace%__Sass__c": "@import \"variables\";\n@import \"community-variables\";\n\n.vlocity {\n    .community {\n        /* Account details styles */\n        .account-card {\n            background-color: $white;\n            padding:15px;\n            margin-bottom:20px;\n            border:1px solid #dedede;\n            .card-top {\n                padding-left:10px;\n                h1 {\n                    margin:0;\n                    color:#4a4a4a;\n                }\n                .account-number {\n                    font-size:12px;\n                    color:#4a4a4a;\n                }\n                .rightActions {\n                    float:right;\n                    color:$font-gray;\n                    cursor:pointer;\n                    i {\n                        color: #05A6DF;\n                    }\n                    .actions {\n                        margin-right:-15px;\n                        i {\n                            font-size:30px;\n                        }\n                    }\n                }\n                .showHideParent {\n                    display:inline-block;\n                    &.hideMore {\n                        .hideButton {\n                            display:inline-block;\n                        }\n                        .showButton {\n                            display:none;\n                        }\n                    }\n                    .hideButton {\n                        display:none;\n                    }\n                    \n                }\n            }\n            .showMore {\n                transition: max-height 0.3s ease-in-out;\n                max-height:0;\n                overflow: hidden;\n                &.show {\n                    max-height:600px;\n                    overflow-y:scroll;\n                }\n                ul {\n                    overflow:auto;\n                    padding:10px 0px;\n                    margin-bottom:10px;\n                    background-color: #f5f6f7;\n                    li {\n                        list-style-type: none;\n                        label {\n                            color:$font-gray;\n                            width:150px;\n                            text-transform: uppercase;\n                        }\n                        .asset-image {\n                            margin:5px 0px;\n                            img {\n                                width:100px;\n                                height:50px;\n                            }\n                        }\n                    }\n                }\n            }\n            .card-info {\n                padding-top:10px;\n                transition: all 0.3s ease-in-out;\n                ul {\n                    &.details {\n                        overflow:auto;\n                        clear:both;\n                        padding:0;\n                        transition: max-height 0.3s ease-in-out;\n                        max-height:600px;\n                        overflow-y:scroll;\n                        li {\n                            list-style-type: none;\n                            float:left;\n                            margin-bottom:20px;\n                            &.detail {\n                                .detail-label {\n                                    text-transform: uppercase;\n                                    color:$font-gray;\n                                    display:block;\n                                    margin:0;\n                                }\n                            }\n                        }\n                    }\n                    @media (max-width: $tablet-width) {\n                        &.details {\n                            border-top:1px solid #dcdce4;\n                            padding-top:10px;\n                        }\n                    }\n                    \n                }\n                .actions-wrapper {\n                    ul {\n                        &.actions {\n                            overflow:hidden;\n                            margin:0;\n                            padding:0;\n                            border-top:1px solid #dcdce4;\n                            li {\n                                list-style-type: none;\n                                float:left;\n                                margin:10px 5px 0px;\n                                padding-right:15px;\n                                border-right:1px solid $separator-gray;\n                                &.last {\n                                    border-right:none;\n                                }\n                                i {\n                                    font-size:20px;\n                                }\n                                .title {\n                                    color:$font-gray;\n                                    line-height: 20px;\n                                    vertical-align: top;\n                                }\n                            }\n                        }\n                    }\n                }\n                @media (max-width: $tablet-width) {\n                    .actions-wrapper {\n                        position: fixed;\n                        bottom: 0px;\n                        right: 0px;\n                        left: 0px;\n                        background-color: transparent;\n                        border-top: none;\n                        transition: all 0.3s ease-in-out;\n                        .page-cover-layer {\n                            position: fixed;\n                            top: 0;\n                            left: 0;\n                            height: 100%;\n                            width: 100%;\n                            z-index: 5;\n                            background-color: rgba(0,0,0,0.5);\n                        }\n                        .actions-container {\n                            position:relative;\n                            z-index:10;\n                            .actions-cancel {\n                                margin-top:10px;\n                                padding:12px;\n                                text-align:center; \n                                color:#05A6DF;\n                                background-color:$white;\n                            }\n                            ul {\n                                background-color:$white;\n                                &.actions {\n                                    li {\n                                        display: block;\n                                        float: none;\n                                        border-right: none;\n                                        border-bottom: 1px solid $separator-gray;\n                                        margin: 0px 10px;\n                                        a {\n                                            padding: 12px 5px;\n                                            display:block;\n                                            .title {\n                                                color: #4a4a4a;\n                                            }\n                                        }\n                                    }\n                                }\n                            }\n                        }\n                    }\n                }\n            }\n        }\n    }\n}",
