            "%vlocity_namespace%__CSS__c": "/*** VARIABLES ***/\n/*** COMMUNITY VARIABLES ***/\n/* Breakpoints */\n/* ipad mini/ipad width is 768px */\n.vlocity .community {\n  /* Account details styles */ }\n  .vlocity .community .account-card {\n    background-color: #ffffff;\n    padding: 15px;\n    margin-bottom: 20px;\n    border: 1px solid #dedede; }\n    .vlocity .community .account-card .card-top {\n      padding-left: 10px; }\n      .vlocity .community .account-card .card-top h1 {\n        margin: 0;\n        color: #4a4a4a; }\n      .vlocity .community .account-card .card-top .account-number {\n        font-size: 12px;\n        color: #4a4a4a; }\n      .vlocity .community .account-card .card-top .rightActions {\n        float: right;\n        color: #9a9a9a;\n        cursor: pointer; }\n        .vlocity .community .account-card .card-top .rightActions i {\n          color: #05A6DF; }\n        .vlocity .community .account-card .card-top .rightActions .actions {\n          margin-right: -15px; }\n          .vlocity .community .account-card .card-top .rightActions .actions i {\n            font-size: 30px; }\n      .vlocity .community .account-card .card-top .showHideParent {\n        display: inline-block; }\n        .vlocity .community .account-card .card-top .showHideParent.hideMore .hideButton {\n          display: inline-block; }\n        .vlocity .community .account-card .card-top .showHideParent.hideMore .showButton {\n          display: none; }\n        .vlocity .community .account-card .card-top .showHideParent .hideButton {\n          display: none; }\n    .vlocity .community .account-card .showMore {\n      transition: max-height 0.3s ease-in-out;\n      max-height: 0;\n      overflow: hidden; }\n      .vlocity .community .account-card .showMore.show {\n        max-height: 600px;\n        overflow-y: scroll; }\n      .vlocity .community .account-card .showMore ul {\n        overflow: auto;\n        padding: 10px 0px;\n        margin-bottom: 10px;\n        background-color: #f5f6f7; }\n        .vlocity .community .account-card .showMore ul li {\n          list-style-type: none; }\n          .vlocity .community .account-card .showMore ul li label {\n            color: #9a9a9a;\n            width: 150px;\n            text-transform: uppercase; }\n          .vlocity .community .account-card .showMore ul li .asset-image {\n            margin: 5px 0px; }\n            .vlocity .community .account-card .showMore ul li .asset-image img {\n              width: 100px;\n              height: 50px; }\n    .vlocity .community .account-card .card-info {\n      padding-top: 10px;\n      transition: all 0.3s ease-in-out; }\n      .vlocity .community .account-card .card-info ul.details {\n        overflow: auto;\n        clear: both;\n        padding: 0;\n        transition: max-height 0.3s ease-in-out;\n        max-height: 600px;\n        overflow-y: scroll; }\n        .vlocity .community .account-card .card-info ul.details li {\n          list-style-type: none;\n          float: left;\n          margin-bottom: 20px; }\n          .vlocity .community .account-card .card-info ul.details li.detail .detail-label {\n            text-transform: uppercase;\n            color: #9a9a9a;\n            display: block;\n            margin: 0; }\n      @media (max-width: 767px) {\n        .vlocity .community .account-card .card-info ul.details {\n          border-top: 1px solid #dcdce4;\n          padding-top: 10px; } }\n      .vlocity .community .account-card .card-info .actions-wrapper ul.actions {\n        overflow: hidden;\n        margin: 0;\n        padding: 0;\n        border-top: 1px solid #dcdce4; }\n        .vlocity .community .account-card .card-info .actions-wrapper ul.actions li {\n          list-style-type: none;\n          float: left;\n          margin: 10px 5px 0px;\n          padding-right: 15px;\n          border-right: 1px solid #dcdce4; }\n          .vlocity .community .account-card .card-info .actions-wrapper ul.actions li.last {\n            border-right: none; }\n          .vlocity .community .account-card .card-info .actions-wrapper ul.actions li i {\n            font-size: 20px; }\n          .vlocity .community .account-card .card-info .actions-wrapper ul.actions li .title {\n            color: #9a9a9a;\n            line-height: 20px;\n            vertical-align: top; }\n      @media (max-width: 767px) {\n        .vlocity .community .account-card .card-info .actions-wrapper {\n          position: fixed;\n          bottom: 0px;\n          right: 0px;\n          left: 0px;\n          background-color: transparent;\n          border-top: none;\n          transition: all 0.3s ease-in-out; }\n          .vlocity .community .account-card .card-info .actions-wrapper .page-cover-layer {\n            position: fixed;\n            top: 0;\n            left: 0;\n            height: 100%;\n            width: 100%;\n            z-index: 5;\n            background-color: rgba(0, 0, 0, 0.5); }\n          .vlocity .community .account-card .card-info .actions-wrapper .actions-container {\n            position: relative;\n            z-index: 10; }\n            .vlocity .community .account-card .card-info .actions-wrapper .actions-container .actions-cancel {\n              margin-top: 10px;\n              padding: 12px;\n              text-align: center;\n              color: #05A6DF;\n              background-color: #ffffff; }\n            .vlocity .community .account-card .card-info .actions-wrapper .actions-container ul {\n              background-color: #ffffff; }\n              .vlocity .community .account-card .card-info .actions-wrapper .actions-container ul.actions li {\n                display: block;\n                float: none;\n                border-right: none;\n                border-bottom: 1px solid #dcdce4;\n                margin: 0px 10px; }\n                .vlocity .community .account-card .card-info .actions-wrapper .actions-container ul.actions li a {\n                