            "%vlocity_namespace%__CSS__c": "/*** VARIABLES ***/\n/*** VARIABLES ***/\n/*** MIXINS ***/\n/*** MIXINS ***/\n/*** STYLES ***/\n.vlocity section.console-card.active {\n  background-color: #FFFFFF; }\n  .vlocity section.console-card.active .card-top {\n    padding: 15px 20px 7px;\n    border-bottom: 2px solid #f2f2f2;\n    cursor: pointer; }\n    .vlocity section.console-card.active .card-top:before, .vlocity section.console-card.active .card-top:after {\n      content: \" \";\n      display: table; }\n    .vlocity section.console-card.active .card-top:after {\n      clear: both; }\n    .vlocity section.console-card.active .card-top h2 {\n      margin: 0 0 4px 0;\n      text-transform: uppercase;\n      font: normal 300 14px/18px \"proxima-nova\", sans-serif;\n      font-size: 28px;\n      line-height: 28px;\n      height: 28px;\n      overflow: hidden;\n      text-overflow: ellipsis;\n      white-space: nowrap;\n      cursor: default;\n      display: inline-block;\n      max-width: 100%; }\n    .vlocity section.console-card.active .card-top span {\n      letter-spacing: 0.02em; }\n      .vlocity section.console-card.active .card-top span.account-number {\n        float: left;\n        font-size: 14px;\n        font-weight: 600; }\n      .vlocity section.console-card.active .card-top span.expires {\n        float: right;\n        font-size: 12px;\n        position: relative;\n        text-transform: uppercase; }\n        .vlocity section.console-card.active .card-top span.expires i.icon {\n          color: #eac438;\n          font-size: 22px;\n          position: absolute;\n          top: -3px;\n          left: -32px; }\n          .vlocity section.console-card.active .card-top span.expires i.icon + .tooltip .tooltip-inner {\n            text-transform: none; }\n  .vlocity section.console-card.active .card-info {\n    padding: 5px 20px; }\n    .vlocity section.console-card.active .card-info:before, .vlocity section.console-card.active .card-info:after {\n      content: \" \";\n      display: table; }\n    .vlocity section.console-card.active .card-info:after {\n      clear: both; }\n    .vlocity section.console-card.active .card-info ul {\n      list-style: none;\n      padding: 0;\n      margin: 0; }\n      .vlocity section.console-card.active .card-info ul.details {\n        float: left;\n        padding: 10px 5px 0px 0px;\n        width: 48%;\n        /* fallback */\n        width: calc(100% - 150px);\n        cursor: pointer;\n        word-wrap: break-word; }\n        .vlocity section.console-card.active .card-info ul.details li {\n          margin-bottom: 16px; }\n          .vlocity section.console-card.active .card-info ul.details li:last-of-type {\n            margin: 0; }\n        .vlocity section.console-card.active .card-info ul.details span {\n          display: block; }\n          .vlocity section.console-card.active .card-info ul.details span.detail-label {\n            font-weight: 600;\n            font-size: 12px;\n            text-transform: uppercase; }\n          .vlocity section.console-card.active .card-info ul.details span.value {\n            text-transform: none;\n            color: #9999a7;\n            font-size: 14px;\n            font-weight: 300; }\n      .vlocity section.console-card.active .card-info ul.actions {\n        float: right;\n        width: 150px;\n        word-wrap: break-word; }\n        .vlocity section.console-card.active .card-info ul.actions a {\n          display: table;\n          width: 100%;\n          height: 53px;\n          border-bottom: 2px solid #f2f2f2; }\n          .vlocity section.console-card.active .card-info ul.actions a img {\n            width: 29px;\n            height: 29px;\n            margin: 5px 9px;\n            display: table-cell;\n            vertical-align: middle; }\n          .vlocity section.console-card.active .card-info ul.actions a i.icon {\n            display: table-cell;\n            vertical-align: middle;\n            font-size: 25px;\n            line-height: 34px;\n            color: #05a6df;\n            width: 40px; }\n            .vlocity section.console-card.active .card-info ul.actions a i.icon:before {\n              font-weight: 100;\n              line-height: 34px; }\n          .vlocity section.console-card.active .card-info ul.actions a span {\n            font-size: 12px;\n            color: #9999a7;\n            line-height: 1;\n            display: table-cell;\n            vertical-align: middle; }\n          .vlocity section.console-card.active .card-info ul.actions a:hover {\n            text-decoration: none; }\n            .vlocity section.console-card.active .card-info ul.actions a:hover i.icon {\n              color: #008ab3; }\n            .vlocity section.console-card.active .card-info ul.actions a:hover span {\n              color: #05a6df; }\n        .vlocity section.console-card.active .card-info ul.actions li:last-of-type a {\n          border: none; }",
