            "%vlocity_namespace%__Sass__c": "/*** VARIABLES ***/\n@import \"variables\";\n\n/*** MIXINS ***/\n@import \"mixin\";\n\n/*** STYLES ***/\n\n.vlocity {\n\n    a {\n        &:hover, &:active, &:focus, &:visited {\n            text-decoration: none;\n        }\n    }\n\n    section.big-console-card {\n        &.active {\n            float: left;\n            width: 31.5165%;\n            width: calc(100% - 14px);\n            margin: 10px;\n            min-height: 360px;\n            background-color: $white;\n            box-shadow: 0 0 5px 0 rgba(0,0,0,.15);\n    \n            .card-top {\n                padding: 15px 20px 7px;\n                // border-bottom: 2px solid $light-gray;\n                cursor:pointer;\n                @include clearfix;\n    \n                h2 {\n                    margin: 0 0 4px 0;\n                    text-transform: uppercase;\n                    font: $font-light;\n                    font-size: $font-heading-size;\n                    line-height: $font-heading-size;\n                    height: 28px;\n                    overflow: hidden;\n                    text-overflow: ellipsis;\n                    white-space: nowrap;\n                    cursor: default;\n                    display: inline-block;\n                    max-width: 100%;\n                }\n    \n                span {\n                    letter-spacing: 0.02em;\n    \n                    &.account-number {\n                        float: left;\n                        font-size: 14px;\n                        font-weight: 600;\n                    }\n    \n                    &.expires {\n                        float: right;\n                        font-size: 12px;\n                        position: relative;\n    \n                        i.icon {\n                            color: $warning;\n                            font-size: 22px;\n                            position: absolute;\n                            top: -3px;\n                            left: -32px;\n    \n                            &+ .tooltip .tooltip-inner {\n                                text-transform: none;\n                            }\n                        }\n                    }\n                }\n            }\n    \n            .card-info {\n                @include clearfix;\n                padding: 5px 20px;\n                \n                .column {\n                    float: left;\n                    padding: 10px 20px;\n                    width: 33.33%;\n                    border-right: 2px solid $light-gray;\n                    height: 100vh;\n                    overflow-y: auto;\n                    \n                    &:first-child {\n                        padding-left: 0px;\n                    }\n                    \n                    &:last-child {\n                        border-right: none;\n                        padding-right: 0px;\n                    }\n                }\n    \n                ul {\n                    list-style: none;\n                    padding: 0;\n                    margin: 0;\n    \n                    &.details {\n                        float: left;\n                        padding: 10px 5px 0px 0px;\n                        width: 100%;\n                        cursor:pointer;\n                        li {\n                            margin-bottom: 16px;\n    \n                            &:last-of-type {\n                                margin: 0;\n                            }\n                        }\n    \n                        span {\n                            display: block;\n    \n                            &.detail-label {\n                                font-weight: 600;\n                                font-size: 12px;\n                            }\n    \n                            &.value {\n                                text-transform: none;\n                                color: $text-gray;\n                                font-size: 14px;\n                                font-weight: 300;\n                            }\n                        }\n                    }\n    \n                    &.actions {\n                        float: left;\n                        width: 100%;\n    \n                        a {\n                            display: table;\n                            width: 100%;\n                            height: 63px;\n                            // border-bottom: 2px solid $light-gray;\n                            border-top: 2px solid $light-gray;\n    \n                            img {\n                                width: 29px;\n                                height: 29px;\n                                margin: 5px 9px;\n                                display: table-cell;\n                                vertical-align: middle;\n                            }\n    \n                            i.icon {\n                                display: table-cell;\n                                vertical-align: middle;\n                                font-size: 35px;\n                                line-height: 34px;\n                                color: $light-blue;\n                                width: 50px;\n                                \n                                &:before {\n                                    font-weight: 100;\n                                    line-height: 34px;\n                                }\n                            }\n    \n                            span {\n                                font-size: 12px;\n                                color: $text-gray;\n                                padding-left: 5px;\n                                line-height: 1;\n                                display: table-cell;\n                                vertical-align: middle;\n    \n                            }\n    \n                            &:hover {\n                                text-decoration: none;\n    \n                                i.icon {\n                                    color: $dark-blue;\n                                }\n    \n                                span {\n                                    color: $light-blue;\n                                }\n                            }\n                        }\n    \n                        li:last-of-type {\n                            a {\n                                border: none;\n                            }\n                        }                   \n                    }\n                }            \n            }\n        }\n    }\n}",
            "%vlocity_namespace%__HTML__c": "<section class=\"big-console-card active\" data-card=\"card-{{cardIndex}}\" ng-class=\"{selected:isSelected(obj.Id)}\">\n    <div class=\"card-top\" ng-click=\"performFlyout(obj.Id, 'card-'+cardIndex)\">\n        <div>\n            <h1 tooltip-placement=\"bottom\" tooltip=\"{{obj.Name}}\">\n                {{obj.Name}}\n            </h1>\n        </div>\n        <!--<span class=\"account-number\">{{obj.Name}}</span>-->\n        \n        <!--<span class=\"expires\">-->\n        <!--    <i ng-if=\"expiresSoon(card.expires, 30)\" tooltip=\"Policy Expires Soon\" class=\"icon icon-v-warning-cirlce\"></i>-->\n        <!--    Expires {{obj.LastModifiedDate | date:'MM/dd/yy'}}</span>-->\n    </div>\n    <div class=\"card-info\">\n        <div class=\"column\">\n            <h2>Details</h2>\n            <ul class=\"details\" ng-click=\"performFlyout(obj.Id, 'card-'+cardIndex)\">\n                <li class=\"detail\" ng-repeat=\"field in data.fields\" ng-show=\"key != 'attributes'\">\n                    <span class=\"detail-label\">{{field.label}}</span>\n                    <span class=\"value\" ng-if=\"!obj[field.relationshipName]\">\n                        <!--{{obj[field.name]}}-->\n                        {{obj | getter: field}}\n                    </span>\n                    <span class=\"value\" ng-if=\"obj[field.relationshipName]\">{{obj[field.relationshipName]['Name']}}</span>\n                </li>\n            </ul>\n        </div>\n        <div class=\"column\">\n            <h2>Actions</h2>\n            <ul class=\"actions\">\n                <li ng-repeat=\"action in data.actions | limitTo: (data.limitActionLinksTo?data.limitActionLinksTo:5)\">\n                    <a href=\"#\" ng-click=\"performAction(action)\">\n                        <i ng-if=\"!action.imageRef\" class=\"icon {{action.vlocityIcon}}\"></i>\n                        <span ng-if=\"action.imageRef\">\n                            <img ng-src=\"{{action.imageRef}}\" alt=\"{{action.displayName}}\"/>\n                        </span>\n                        <span>{{action.displayName}}</span>\n                    </a>\n                </li>\n            </ul>\n        </div>\n        <div class=\"column\" ng-if=\"data.flyout.layout\">\n            <h2>Devices</h2>\n            <vloc-layout layout-name=\"{{data.flyout.layout}}\"></vloc-layout>\n        </div>\n    </div>\n</section>",
