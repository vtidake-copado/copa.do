reen and (max-width: 450px) {\n  .via-slds .vloc-ins-coverages-container .vloc-ins-coverages-card .vloc-ins-coverages-card-inner-wrapper .vloc-ins-coverages-header-container {\n    padding-left: 1rem;\n    padding-right: 1rem;\n    position: relative;\n    padding-bottom: 2rem; }\n    .via-slds .vloc-ins-coverages-container .vloc-ins-coverages-card .vloc-ins-coverages-card-inner-wrapper .vloc-ins-coverages-header-container .vloc-ins-coverages-header .slds-media__figure {\n      margin-right: 0.75rem; }\n    .via-slds .vloc-ins-coverages-container .vloc-ins-coverages-card .vloc-ins-coverages-card-inner-wrapper .vloc-ins-coverages-header-container .vloc-ins-coverages-header .vloc-ins-coverage-custom-image {\n      width: 1.5rem; }\n    .via-slds .vloc-ins-coverages-container .vloc-ins-coverages-card .vloc-ins-coverages-card-inner-wrapper .vloc-ins-coverages-header-container .vloc-ins-coverages-header .vloc-ins-coverages-child-name {\n      font-size: 16px; }\n    .via-slds .vloc-ins-coverages-container .vloc-ins-coverages-card .vloc-ins-coverages-card-inner-wrapper .vloc-ins-coverages-header-container .vloc-ins-coverages-header .vloc-ins-coverages-child-description {\n      font-size: 12px; }\n  .via-slds .vloc-ins-coverages-container .vloc-ins-coverages-card .vloc-ins-coverages-card-inner-wrapper .vloc-ins-coverages-total {\n    font-size: 14px; }\n  .via-slds .vloc-ins-coverages-container .vloc-ins-coverages-card .vloc-ins-coverages-coverage-limit .slds-select {\n    font-size: 14px; } }\n\n@media screen and (max-width: 350px) {\n  .via-slds .vloc-ins-coverages-container .vloc-ins-coverages-card .vloc-ins-coverages-card-inner-wrapper .vloc-ins-coverages-header-container {\n    padding-left: 0.75rem;\n    padding-right: 0.75rem; } }\n",
                        "%vlocity_namespace%__CustomJavascript__c": "// Instantiating sortedCoverages array that will contain data repeated over in the UI\nbaseCtrl.prototype.$scope.sortedCoverages = [];\n// Template Config Options:\nbaseCtrl.prototype.$scope.insCoveragesConfig = {\n    showParentProduct: false, // Show Parent Product in UI\n    remoteMethod: false, // Boolean to decide whether we can call buttonClick from JS (set to true in setSelectedOption)\n    callButtonClick: true, // Manually turn the automatic remote call off even if the class and method are set\n    coverageAccordion: true, // Turn on/off accordion on coverages\n    coverageAccordionMinAttrs: 2, // The number of minimum coverage attributes needed (throughout all categories) to trigger an accordion if attrCatAccordion is false\n    attrCatAccordion: false, // Turn on/off accordion for attribute categories (will automatically be off if only 1 category)\n    attrCatAccordionMinCats: 2 // The number of minimum attribute categories needed to trigger a category accordion if attrCatAccordion is true\n};\n\n// Helper function to loop through service result\nfunction loopThroughAttributes(products) {\n    var i, j, k;\n    var selectedCount = 0;\n    for (i = 0; i < products.length; i++) {\n        products[i].originalIndex = i;\n        if (products[i].attributeCategories) {\n            attrCats = products[i].attributeCategories.records;\n            if (attrCats) {\n                if (!products[i].numberCategories) {\n                    products[i].numberCategories = attrCats.length;\n                } else {\n                    products[i].numberCategories += attrCats.length;\n                }\n                for (j = 0; j < attrCats.length; j++) {\n                    if (attrCats[j].productAttributes) {\n                        prodAttrs = attrCats[j].productAttributes.records;\n                        if (prodAttrs) {\n                            if (!products[i].numberAttributes) {\n                                products[i].numberAttributes = prodAttrs.length;\n                            } else {\n                                products[i].numberAttributes += prodAttrs.length; \n                            }\n                            for (k = 0; k < prodAttrs.length; k++) {\n                                prodAttrs[k].originalProductIndex = i;\n                                prodAttrs[k].originalCategoryIndex = j;\n                                prodAttrs[k].originalAttributeIndex = k;\n                                if (prodAttrs[k].userValues.length) {\n                                    selectedCount = 0;\n                                    angular.forEach(prodAttrs[k].userValues, function(userValue, idx) {\n                                        angular.forEach(userValue, function(keyValue, key) {\n                                            if (keyValue) {\n                                                selectedCount++;\n                                            }\n                                        });\n                                    });\n                                    prodAttrs[k].multiSelectLabel = selectedCount + ' Selected';\n                                }\n                                if (prodAttrs[k].values) {\n                                    angular.forEach(prodAttrs[k].values, function(value, i) {\n                                        if (value.descriptor) {\n                                            value.label = value.label + ' - ' + value.descriptor;\n                                        }\n                                    });\n                                }\n                            }\n                        }\n                    }\n                }\n            }\n        }\n    }\n    return products;\n}\n\n// Format data so it can be repeated over in the UI (called on init)\nfunction formatData(products) {\n    var i, j, attrCats, prodAttrs;\n    var foundFirstOptional = false;\n    var productsReference = loopThroughAttributes(angular.copy(products));\n    var coverages = loopThroughAttributes(angular.copy(productsReference[0].childProducts.records));\n    baseCtrl.prototype.$scope.sortedCoverages = [];\n\n    if (coverages && !baseCtrl.prototype.$scope.sortedCoverages.length) {\n        baseCtrl.prototype.$scope.sortedCoverages = coverages.sort(function(x, y) {\n            if (x.isOptional === y.isOptional) {\n                if (x.Name < y.Name) {\n                    return -1;\n                } else {\n                    return 1;\n                }\n            } else if (x.isOptional) {\n                return 1;\n            } else {\n                return -1;\n            }\n        });\n\n        for (j = 0; j < baseCtrl.prototype.$scope.sortedCoverages.length; j++) {\n            if (baseCtrl.prototype.$scope.sortedCoverages[j].isOptional && !foundFirstOptional) {\n                baseCtrl.prototype.$scope.sortedCoverages[j].firstOptional = true;\n                foundFirstOptional = true;\n            }\n        }\n        // Can't have childProducts or it creates an infinite loop of childProducts since\n        // the parent product is at the top of the sortedCoverages array\n        delete productsReference[0].childProducts;\n        baseCtrl.prototype.$scope.sortedCoverages.unshift(productsReference[0]);\n        baseCtrl.prototype.$scope.sortedCoverages[0].parentProduct = true;\n    }\n    console.log('baseCtrl.prototype.$scope.sortedCoverages', baseCtrl.prototype.$scope.sortedCoverages);\n}\n\n// This function is used in place of formatData for a different display. Both can't be used at the same time\nfunction organizeDataByCategory(products) {\n    var productsReference, coverages;\n    if (localStorage.vlocInsCoveragesCategories) {\n        baseCtrl.prototype.$scope.categories = angular.fromJson(localStorage.getItem('vlocInsCoveragesCategories'));\n        delete localStorage.vlocInsCoveragesCategories;\n        return;\n    } else {\n        productsReference = loopThroughAttributes(angular.copy(products));\n        coverages = loopThroughAttributes(angular.copy(productsReference[0].childProducts.records));\n        baseCtrl.prototype.$scope.categories = {};\n        if (products && products.length && products.length > 1) {\n            console.log('There is 0 or more than one parent product. This template will only work with 1 parent product.');\n        } else {\n            // First grab all the Attribute Categories\n            angular.forEach(coverages, function(product, i) {\n                var attrCats = product.attributeCategories.records;\n                if (attrCats && attrCats.length) {\n                    angular.forEach(attrCats, function(category, j) {\n                        if (!(category.Code__c in baseCtrl.prototype.$scope.categories)) {\n                            baseCtrl.prototype.$scope.categories[category.Code__c] = {\n                                code: category.Code__c,\n                                name: category.Name,\n                                displaySequence: category.displaySequence,\n                                id: category.id\n                            };\n                        }\n                        if (!baseCtrl.prototype.$scope.categories[category.Code__c].products) {\n                            if (product.RecordTypeName__c === 'CoverageSpec') {\n                                baseCtrl.prototype.$scope.categories[category.Code__c].products = [];\n                                baseCtrl.prototype.$scope.categories[category.Code__c].products.push(product);\n                            }\n                        } else {\n                            if (product.RecordTypeName__c === 'CoverageSpec') {\n                                baseCtrl.prototype.$scope.categories[category.Code__c].products.push(product);\n                            }\n                        }\n                    });\n                }\n            });\n            console.log('baseCtrl.prototype.$scope.categories', baseCtrl.prototype.$scope.categories);\n        }\n    }\n}\n\nbaseCtrl.prototype.setSelectedOption = function(control, p, option, index, scp, bSelected) {\n    if (control === undefined || control === null) {\n        return;\n    }\n    console.log('%c' + p.Name + ' Data:', 'font-size: 14px; color: aqua; font-style: italic;', p);\n    control.response = control.vlcSI[control.itemsKey];\n    // formatData(control.vlcSI[control.itemsKey]);\n    organizeDataByCategory(control.vlcSI[control.itemsKey]);\n    if (control.propSetMap.dataJSON === true) {\n        scp.aggregate(scp, control.index, control.indexInParent, true, -1);\n    }\n    if (control.propSetMap.remoteClass && control.propSetMap.remoteMethod) {\n        baseCtrl.prototype.$scope.insCoveragesConfig.remoteMethod = true;\n    }\n};\n\n// Calls OmniScript buttonClick function, will perform remote action defined on the Seletable Item\n// that houses this template\nbaseCtrl.prototype.changeCoverage = function(response, control, scp, product, attribute, currentTextValue) {\n    var currentProduct;\n    // Save current state of categories to localStorage\n    // This prevents us having to parse the JSON response on each recalc\n    // Also saves the open state of each collapseable category\n    localStorage.setItem('vlocInsCoveragesCategories', angular.toJson(baseCtrl.prototype.$scope.categories));\n    if (product.parentProduct) {\n        currentProduct = control.vlcSI[control.itemsKey][0];\n    } else {\n        currentProduct = control.vlcSI[control.itemsKey][0].childProducts.records[attribute.originalProductIndex];\n    }\n    currentProduct.attributeCategories.records[attribute.originalCategoryIndex].productAttributes.records[attribute.originalAttributeIndex].userValues = attribute.userValues;\n    if (currentTextValue) {\n        if (attribute.userValues && attribute.userValues !== currentTextValue) {\n            if (baseCtrl.prototype.$scope.insCoveragesConfig.callButtonClick && baseCtrl.prototype.$scope.insCoveragesConfig.remoteMethod) {\n                scp.buttonClick(response, control, scp);\n            }\n        }\n    } else {\n        if (baseCtrl.prototype.$scope.insCoveragesConfig.callButtonClick && baseCtrl.prototype.$scope.insCoveragesConfig.remoteMethod) {\n            scp.buttonClick(response, control, scp);\n        }\n    }\n};\n\nbaseCtrl.prototype.selectOptionalCoverage = function(child, product, response, control, scp) {\n    if (!child.isOptional) {\n        console.log('not optional', child);\n        return;\n    } else {\n        child.isSelected = !child.isSelected;\n        product.childProducts.records[child.originalIndex].isSelected = !product.childProducts.records[child.originalIndex].isSelected;\n        if (baseCtrl.prototype.$scope.insCoveragesConfig.callButtonClick && baseCtrl.prototype.$scope.insCoveragesConfig.remoteMethod) {\n            scp.buttonClick(response, control, scp);\n        }\n    }\n};\n\nbaseCtrl.prototype.doAccordion = function(child) {\n    if (!baseCtrl.prototype.$scope.insCoveragesConfig.coverageAccordion) {\n        return false;\n    } else if (child.parentProduct) {\n        return false;\n    } else {\n        if (!baseCtrl.prototype.$scope.insCoveragesConfig.attrCatAccordion) {\n            if (child.numberAttributes < baseCtrl.prototype.$scope.insCoveragesConfig.coverageAccordionMinAttrs) {\n                return false;\n            } else {\n                return true;\n            }\n        } else {\n            if (child.numberCategories < baseCtrl.prototype.$scope.insCoveragesConfig.attrCatAccordionMinCats) {\n                return false;\n            } else {\n                return true;\n            }\n        }\n    }\n};\n\nbaseCtrl.prototype.addCheckboxValue = function(attribute, value, response, control, scp, child, attribute) {\n    var selectedCount = 0;\n    angular.forEach(attribute.userValues, function(userValue, i) {\n        if (value.value in userValue) {\n            userValue[value.value] = !userValue[value.value];\n        }\n        angular.forEach(userValue, function(keyValue, key) {\n            if (keyValue) {\n                selectedCount++;\n            }\n        });\n    });\n    attribute.multiSelectLabel = selectedCount + ' Selected';\n    baseCtrl.prototype.changeCoverage(response, control, scp, child, attribute);\n    console.log('multiselect userValues', attribute.userValues);\n    console.log('baseCtrl.prototype.$scope.sortedCoverages', baseCtrl.prototype.$scope.sortedCoverages);\n};\n\nbaseCtrl.prototype.stopPropagation = function(event) {\n    event.stopPropagation();\n};\n\nvlocity.cardframework.registerModule.directive('insDropdownHandler', function($document) {\n    'use strict';\n    return {\n        restrict: 'A',\n        link: function(scope, element, attrs) {\n            var focused = false;\n            var onClick = function (event) {\n                var isChild = element.has(event.target).length > 0;\n                var isSelf = element[0] == event.target;\n                var isInside = isChild || isSelf;\n                var className = event.target.className;\n                if (event.target.nodeName === 'path') {\n                    className = event.target.parentElement.parentElement.className;\n                } else if (event.target.nodeName === 'svg') {\n                    className = event.target.parentElement.className;\n                }\n                if (!isInside && className.indexOf(attrs.restrictElement) < 0) {\n                    scope.$apply(attrs.insDropdownHandler);\n                    $document.off('click', onClick);\n                    focused = false;\n                }\n            };\n            element.on('focus click', function(event) {\n                if (!focused) {\n                    $document.on('click', onClick);\n                    focused = true;\n                }\n            });\n        }\n    };\n});",
